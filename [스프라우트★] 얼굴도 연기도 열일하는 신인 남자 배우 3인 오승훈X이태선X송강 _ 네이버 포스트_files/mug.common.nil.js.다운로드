(function(window,$){$(document).ready(function(){mug.common.ntmSendContentsViewLog();});$(window).load(function(){var url=location.href;if(url.indexOf("postView")<0||window.__templateType__==null){return;}var event_action="";var event_label="";var oEvent_label=null;if(__templateType__==mug.viewer.templateType.UGC_SIMPLE||__templateType__==mug.viewer.templateType.UGC_CARD){$(".se_subjectMatter").click(function(oEvent){if(jQuery(oEvent.target)!=null){event_action=jQuery(oEvent.target).attr("href");oEvent_label=jQuery(oEvent.target).parent().children(function(v){return v.hasClass("__se_material_title");});}var $evtLabel=jQuery(oEvent_label);if($evtLabel.length>0){event_label=$(".__se_material_title",$evtLabel.first()).text();}if(event_label.trim().length<=0){return;}mug.common.nilSendClickEvent(event_action,event_label);});}else{$(".og.post_add").click(function(oEvent){var elCurrent=this.parentNode.querySelectorAll(".og")[0];var ogTagClass=($(elCurrent).hasClass("post_add_product"))?"tit2":"tit";if(jQuery(oEvent.target)!=null){event_action=jQuery(oEvent.target).attr("href");oEvent_label=jQuery(oEvent.target).parent().children(function(v){return v.hasClass(ogTagClass);});}var $evtLabel=jQuery(oEvent_label);if($evtLabel.length>0){event_label=$evtLabel.first().text();}mug.common.nilSendClickEvent(event_action,event_label);});}$(".se_og_box.__se_link").click(function(oEvent){event_action=$(oEvent.currentTarget).attr("href");$evtLabel=$(oEvent.currentTarget).find(".se_og_tit");if($evtLabel.length>0){event_label=$evtLabel.text();}mug.common.nilSendClickEvent(event_action,event_label);});});if("onpagehide" in window){addEventListener("pagehide",function(event){mug.common.ntmSendLeaveEvent();});}else{addEventListener("beforeunload",function(event){mug.common.ntmSendLeaveEvent();});}})(window,jQuery);